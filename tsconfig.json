{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "es5",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@data/*": ["data/*"],
      "@pages/*": ["pages/*"],
      "@store/*": ["store/*"],
      "@styles/*": ["styles/*"],
      "@layouts/*": ["layouts/*"],
      "@utils/*": ["utils/*"],
      "@webgl/*": ["webgl/*"]
    }
  }
}
